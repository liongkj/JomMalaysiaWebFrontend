@model JomMalaysia.Presentation.Models.Listings.OfficialContact

<div class="form-group row">
    <div class="col-md-3">@Html.DisplayNameFor(m => m.Email)</div>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.Email, new {@class = "form-control contact_field", placeholder = "hello@email.com"})
        @Html.ValidationMessageFor(m => m.Email)
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">@Html.DisplayNameFor(m => m.Website)</div>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.Website, new {@class = "form-control contact_field", placeholder = "Official Website / Facebook"})
        @Html.ValidationMessageFor(m => m.Website)
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">@Html.DisplayNameFor(m => m.MobileNumber)</div>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.MobileNumber, new {@class = "form-control contact_field", placeholder = "Phone"})
        @Html.ValidationMessageFor(m => m.MobileNumber)
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">@Html.DisplayNameFor(m => m.OfficeNumber)</div>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.OfficeNumber, new {@class = "form-control contact_field", placeholder = "Office"})
        @Html.ValidationMessageFor(m => m.OfficeNumber)
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">@Html.DisplayNameFor(m => m.Fax)</div>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.Fax, new {@class = "form-control contact_field", placeholder = "Fax"})
        @Html.ValidationMessageFor(m => m.Fax)
    </div>
</div>