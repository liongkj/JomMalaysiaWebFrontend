

<script src="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script>
<script>
$(document).ready(function(){
    $('input.timepicker.start-time').timepicker({
        timeFormat: 'HHmm',
        dropdown: true,
        scrollbar: true,
        defaultTime: '8',
        interval: 30,
        zindex: 9999999
    });
    
    $('input.timepicker.close-time').timepicker({
            timeFormat: 'HHmm',
            dropdown: true,
            scrollbar: true,
            defaultTime: '5',
            interval: 30,
            zindex: 9999999
        });
});
</script>